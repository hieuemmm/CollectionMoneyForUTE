<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg">

    <ImageView
        android:id="@+id/profile_box"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.589"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/box_canhan" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/Candidate_Button_DoanPhi"
        android:layout_width="366dp"
        android:layout_height="48dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="20dp"
        android:background="@drawable/bg_button_long"
        android:text="Đăng Xuất"
        android:textColor="@color/white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.511"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/profile_box" />

    <TextView
        android:id="@+id/profile_username"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="210dp"
        android:text="Admin"
        android:textColor="@color/black"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="@+id/profile_box"
        app:layout_constraintStart_toStartOf="@+id/profile_box"
        app:layout_constraintTop_toTopOf="@+id/profile_box" />

    <TextView
        android:id="@+id/profile_role"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:text="Vai trò cảu bạn : Admin"
        app:layout_constraintEnd_toEndOf="@+id/profile_box"
        app:layout_constraintStart_toStartOf="@+id/profile_box"
        app:layout_constraintTop_toBottomOf="@+id/profile_username" />

    <TextView
        android:id="@+id/profile_thongke"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="28dp"
        android:text="Thống kê:"
        android:textColor="@color/purple_700"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@+id/profile_box"
        app:layout_constraintTop_toBottomOf="@+id/profile_role" />

    <TextView
        android:id="@+id/profile_countmembertitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="5dp"
        android:text="Tổng đoàn viên:"
        android:textColor="@color/black"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@+id/profile_box"
        app:layout_constraintTop_toBottomOf="@+id/profile_thongke" />

    <TextView
        android:id="@+id/profile_songuoi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="180dp"
        android:layout_marginTop="56dp"
        android:text="2000 nguười"
        android:textColor="@color/black"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@+id/profile_box"
        app:layout_constraintTop_toBottomOf="@+id/profile_role" />

    <TextView
        android:id="@+id/profile_doanphi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="5dp"
        android:text="Đoàn phí hiện tại:"
        android:textColor="@color/black"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@+id/profile_box"
        app:layout_constraintTop_toBottomOf="@+id/profile_countmembertitle" />

    <TextView
        android:id="@+id/profile_tendot"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="90dp"
        android:layout_marginTop="5dp"
        android:text="Tên đợt:"
        android:textColor="@color/black"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@+id/profile_box"
        app:layout_constraintTop_toBottomOf="@+id/profile_doanphi" />

    <TextView
        android:id="@+id/profile_doanphi1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="180dp"
        android:layout_marginTop="5dp"
        android:text="Đoàn phí 1"
        android:textColor="@color/black"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@+id/profile_box"
        app:layout_constraintTop_toBottomOf="@+id/profile_doanphi" />

    <TextView
        android:id="@+id/profile_soluong"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="80dp"
        android:layout_marginTop="5dp"
        android:text="Số lượng:"
        android:textColor="@color/black"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@+id/profile_box"
        app:layout_constraintTop_toBottomOf="@+id/profile_tendot" />

    <TextView
        android:id="@+id/profile_soluongnguoi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="180dp"
        android:layout_marginTop="5dp"
        android:text="1600/2000 người (90%)"
        android:textColor="@color/black"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@+id/profile_box"
        app:layout_constraintTop_toBottomOf="@+id/profile_tendot" />

    <TextView
        android:id="@+id/profile_tongtien"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="78dp"
        android:layout_marginTop="5dp"
        android:text="Tổng tiền:"
        android:textColor="@color/black"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@+id/profile_box"
        app:layout_constraintTop_toBottomOf="@+id/profile_soluong" />

    <TextView
        android:id="@+id/profile_sotien"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="180dp"
        android:layout_marginTop="5dp"
        android:text="16.000.000VND"
        android:textColor="@color/black"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@+id/profile_box"
        app:layout_constraintTop_toBottomOf="@+id/profile_soluong" />

    <TextView
        android:id="@+id/profile_hoiphi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginTop="5dp"
        android:text="Hội phí hiện tại:"
        android:textColor="@color/black"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@+id/profile_box"
        app:layout_constraintTop_toBottomOf="@+id/profile_tongtien" />

    <TextView
        android:id="@+id/profile_tendot1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="90dp"
        android:layout_marginTop="5dp"
        android:text="Tên đợt:"
        android:textColor="@color/black"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@+id/profile_box"
        app:layout_constraintTop_toBottomOf="@+id/profile_hoiphi" />

    <TextView
        android:id="@+id/profile_hoiphi1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="180dp"
        android:layout_marginTop="5dp"
        android:text="Hội phí 1"
        android:textColor="@color/black"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@+id/profile_box"
        app:layout_constraintTop_toBottomOf="@+id/profile_hoiphi" />

    <TextView
        android:id="@+id/profile_soluong1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="80dp"
        android:layout_marginTop="5dp"
        android:text="Số lượng:"
        android:textColor="@color/black"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@+id/profile_box"
        app:layout_constraintTop_toBottomOf="@+id/profile_tendot1" />

    <TextView
        android:id="@+id/profile_soluongnguoi1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="180dp"
        android:layout_marginTop="5dp"
        android:text="1500/2000 người (70%)"
        android:textColor="@color/black"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@+id/profile_box"
        app:layout_constraintTop_toBottomOf="@+id/profile_tendot1" />

    <TextView
        android:id="@+id/profile_tongtien1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="80dp"
        android:layout_marginTop="5dp"
        android:text="Tổng tiền:"
        android:textColor="@color/black"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@+id/profile_box"
        app:layout_constraintTop_toBottomOf="@+id/profile_soluong1" />

    <TextView
        android:id="@+id/profile_sotien1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="180dp"
        android:layout_marginTop="5dp"
        android:text="350.000 VND"
        android:textColor="@color/black"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@+id/profile_box"
        app:layout_constraintTop_toBottomOf="@+id/profile_soluong1" />

</androidx.constraintlayout.widget.ConstraintLayout>